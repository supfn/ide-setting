<toolSet name="External Tools">
  <tool name="eslint" description="eslint" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="npm" />
      <option name="PARAMETERS" value="run eslint -s" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
    <filter>
      <option name="NAME" value="line" />
      <option name="DESCRIPTION" value="line" />
      <option name="REGEXP" value="$FILE_PATH$: line $LINE$, col $COLUMN$" />
    </filter>
  </tool>
</toolSet>