<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="AngularJS" injector-id="xml">
      <display-name>el-button/@@click</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().matches("@click")).withParent(xmlTag().withLocalName(string().equalTo("el-button")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>